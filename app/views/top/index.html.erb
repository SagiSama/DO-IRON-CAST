          <div class="tabs-container">
            <div class="tabs-left">
              <ul class="nav nav-tabs">
                <% i = 0 %>
                <% @newses.each do |news| %>
                  <% if i == 0 %>
                    <% active = "active" %>
                  <% end %>
                  <li><a class="nav-link <%= active %>" data-toggle="tab" href="#tab-<%= i %>"> <%= news.title %></a></li>
                  <% i = i + 1 %>
                <% end %>
              </ul>
              <div class="tab-content">
                <% i = 0 %>
                <% @newses.each do |news| %>
                  <% if i == 0 %>
                    <% active = "active" %>
                  <% end %>
                  <div id="tab-<%= i %>" class="tab-pane <%= active %>">
                    <div class="panel-body">
                      <p><strong><%= news.title %></strong></p>
                      <% if news.images.attached? %>
                        <% news.images.each do |image| %>
                          <%= image_tag(url_for(image), class: "img-fluid") %>
                        <% end %>
                      <% end %>
                      <p><%= news.subject %></p>
                    </div>
                  </div>
                  <% i = i + 1 %>
                <% end %>
              </div>
            </div>
          </div>
        </div>
      </div>
      </br>
    </div>
    </div>
  </section>

